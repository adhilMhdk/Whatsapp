<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whatsappclone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WhatsappClone"
        android:usesCleartextTraffic="true">
        <activity android:name=".activitys.mainSession.contactsSession.ContactsActivity"></activity>
        <activity android:name=".activitys.mainSession.MainActivity" />
        <activity android:name=".activitys.phoneAuht.PhoneAuthActivityc" />
        <activity android:name=".activitys.phoneAuht.PhoneAuthActivityb" />
        <activity android:name=".activitys.phoneAuht.PhoneAuthActivitia" />
        <activity android:name=".activitys.PhoneAuthActivity" />
        <activity android:name=".activitys.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>